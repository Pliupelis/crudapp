<% for(var i = 0; i<users.length; i++){ %>
<tr>
    <td><%= i+1 %></td>
    <td><%= users[i].name %></td>
    <td><%= users[i].email %></td>
    <td><%= users[i].gender %></td>
    <td><%= users[i].status %></td>
    <td>
        <a href="/update-user?id=<%= users[i]._id %>" class="btn">
            <span class="text"><i class="fas fa-user-edit"></i></span>
        </a>
        <a class="btn icon" data-id=<%= users[i]._id%> >
            <span class="text"><i class="fas fa-trash"></i></span>
        </a>
    </td>
</tr>
<% } %>